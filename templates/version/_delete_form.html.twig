<form  style="display:inline;" method="post" action="{{ path('version_delete', {'id': version.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer cette version?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ version.id) }}">
    <button class="btn btn-info btn-sm mb-2">Supprimer</button>
</form>
